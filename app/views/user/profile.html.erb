<% if @user == current_user.id %>
	<h1>Mein Profil: </h1><br>
	<h2>Freunde: </h2>
		<% if @friends.present? %>
			<ul>
			<% @friends.each do |f| %>
				<li>
					<%= f.first_name %><%= f.last_name %><br>
				</li>
			<% end %>
			</ul><br>
		<% end %>
	<h2>Bilder:</h2><br>
<% else %>
	<% if @is_friend == 1 %>
		<h1>xyzs Profil: </h1><br>
		<h2>Freunde: </h2>
			<% if @friends.present? %>
				<ul>
				<% @friends.each do |f| %>
					<li>
						<%= f.first_name %><%= f.last_name %>
					</li><br>
				<% end %>
				</ul><br>
			<% end %>
		<h2>Bilder:</h2><br>
	<% else %>
		Is no Friend
	<% end %>
	Not my Page
<% end %>

