<%= form_for @image, :html => {:multipart => true} do |f| %>
    <div class="field">
        <%= f.label :title %><br />
        <%= f.text_field :title  %>
    </div>
      <%= f.hidden_field :project_id %>
    <div class="field">
        <%= f.label :content %><br />
        <%= f.text_field :content %>
    </div>
    <div class="field">
        <%= f.file_field :images %>
    </div>
    <div class="actions">
        <%= f.submit %>
    </div>
<% end %>
